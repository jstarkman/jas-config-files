{
    "diffEditor.diffAlgorithm": "advanced",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": "'ss01', 'ss02', 'ss05', 'ss06'",
    "editor.fontWeight": "400",
    "editor.guides.bracketPairs": true,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80,
        88,
        100,
        120
    ],
    "editor.tabSize": 8,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.log.*": "log",
        "*.service": "ini",
        "Jenkinsfile*": "groovy",
        "*.conf.*": "properties",
        ".env*": "dotenv",
        "appsettings.*.json": "jsonc",
        "appsettings.json": "jsonc",
        "app.json": "jsonc",
        "workspace.json": "jsonc"
    },
    "files.autoSave": "off",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "telemetry.telemetryLevel": "off",
    "window.commandCenter": false,
    "window.confirmBeforeClose": "always",
    "window.menuBarVisibility": "compact",
    "window.title": "${rootPath}${separator}${dirty}${activeEditorLong}",
    "window.titleSeparator": " | ",
    "workbench.editorAssociations": {
        "*.hex": "hexEditor.hexedit",
        "*.class": "hexEditor.hexedit",
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.editor.wrapTabs": true,
    "workbench.sideBar.location": "right",
    "workbench.tree.indent": 32,
    "workbench.tree.renderIndentGuides": "always",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-21",
            "path": "/usr/lib/jvm/java-21-openjdk-amd64",
        }
    ],
    "java.configuration.updateBuildConfiguration": "interactive",
    "java.debug.settings.showHex": true,
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-21-openjdk-amd64",
    "maven.executable.options": "-DskipTests=true",
    "maven.terminal.customEnv": [
        {
            "environmentVariable": "JAVA_HOME",
            "value": "/usr/lib/jvm/java-21-openjdk-amd64",
        }
    ],
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "[properties]": {},
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.views.commitDetails.autolinks.enabled": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "keyword.other.dotenv",
                "settings": {
                    "foreground": "#FF000000"
                }
            }
        ]
    },
    "yaml.schemas": {
    },
    "remote.SSH.remotePlatform": {
    }
}
